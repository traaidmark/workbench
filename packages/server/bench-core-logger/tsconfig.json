{
  "compilerOptions": {
   "lib": [
      "es5",
      "es6"
   ],
   "target": "es6",
   "module": "commonjs",
   "esModuleInterop": true,
   "moduleResolution": "node",
   "rootDir": "./src",   
   "outDir": "./dist",
   "emitDecoratorMetadata": true,
   "experimentalDecorators": true,
   "sourceMap": true,
   "baseUrl": ".",
  },
  "include": ["./src"],
  "exclude": ["dist", "build", "node_modules"]
}
